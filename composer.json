{
    "name": "glooby/debug-bundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "description": "Symfony debugging and log bundle",
    "homepage": "https://www.glooby.se",
    "keywords": ["symfony", "bundle", "glooby", "debug", "guzzle", "http", "rest", "logging"],
    "authors": [
        {
            "name": "Emil Kilhage",
            "email": "emil.kilhage@glooby.com",
            "homepage": "https://www.glooby.se"
        }
    ],
    "autoload": {
        "psr-4": {
            "Glooby\\DebugBundle\\": "src/DebugBundle",
            "Glooby\\Debug\\": "src/Debug"
        }
    },
    "require": {
        "php": ">=5.6",
        "guzzlehttp/guzzle": "~5.1"
    },
    "require-dev": {
        "phpspec/prophecy": "~1.5"
    }
}
